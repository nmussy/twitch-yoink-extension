{
  "name": "Twitch chat Yoink badge",
  "description": "Adds a Yoink badge to Twitch chat",
  "version": "1.0.6",
  "manifest_version": 2,
  "icons": {
    "16": "/images/icon16.png",
    "32": "/images/icon32.png"
  },
  "permissions": [
    "webRequest",
    "https://d3ccy3avj75z8y.cloudfront.net/users.txt"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.twitch.tv/*"],
      "js": ["dist/content-script.js"]
    }
  ],
  "web_accessible_resources": ["images/badge.png"]
}
